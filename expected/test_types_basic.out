create materialized view test_rollup1 as (
    select count(*),name
    from test
    group by name
);
create materialized view test_rollup2 as (
    select count(*),name,num
    from test
    group by name,num
);
create materialized view test_rollup3 as (
    select
        name,
        sum(num) as sum,
        count(*) as count_all,
        count(num) count_num,
        max(num),
        min(num)
    from test
    group by name
);
create materialized view test_rollup4 as (
    select 
        sum(num) as sum,
        count(*) as count_all,
        count(num),
        max(num),
        min(num)
    from test
);
